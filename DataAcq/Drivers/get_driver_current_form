import pandas as pd
import requests
import get_driver_race_result

base_url = "https://api.jolpi.ca/ergast/f1/"
def get_driver_current_form(driverId, year):
    url = f"{base_url}{year}/races"
    response = requests.get(url)
    data = response.json()
    races = data['MRData']['RaceTable']['Races']
    
    
